{
  "name": "@carbonteq/hexapp",
  "version": "0.9.0",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "exports": {
    "node": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "/lib"
  ],
  "scope": "@carbonteq",
  "repository": {
    "type": "git",
    "url": "https://github.com/carbonteq/hexapp"
  },
  "bugs": {
    "url": "https://github.com/carbonteq/hexapp/issues"
  },
  "homepage": "https://github.com/carbonteq/hexapp#readme",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "rome check src tests",
    "lint:fix": "rome check src tests --apply-suggested",
    "fmt:dry": "rome format src tests",
    "fmt": "rome format src tests --write",
    "ci": "rome ci src tests",
    "tc": "tsc --pretty --noEmit --project tsconfig.build.json",
    "prebuild": "rimraf lib",
    "build": "rollup -c rollup.config.js",
    "postbuild": "tsc --emitDeclarationOnly --declaration --declarationMap --project tsconfig.build.json",
    "dev": "rollup -c rollup.config.js",
    "test": "jest --verbose",
    "test:dbg": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@swc/core": "^1.3.24",
    "@swc/jest": "^0.2.24",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "changesets": "^1.0.2",
    "jest": "^29.3.1",
    "jest-extended": "^3.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.8.1",
    "rollup-plugin-node-externals": "^5.0.3",
    "rollup-plugin-swc3": "0.8.0",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "rome": "^11.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "oxide.ts": "^1.1.0",
    "zod": "^3.20.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0",
    "yarn": ">=1.22.0"
  },
  "packageManager": "yarn@1.22.19"
}
