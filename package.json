{
  "name": "@carbonteq/hexapp",
  "version": "0.2.0",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "exports": {
    "node": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "/lib"
  ],
  "scope": "@carbonteq",
  "repository": {
    "type": "git",
    "url": "https://github.com/carbonteq/hexapp"
  },
  "bugs": {
    "url": "https://github.com/carbonteq/hexapp/issues"
  },
  "homepage": "https://github.com/carbonteq/hexapp#readme",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "eslint \"{src,tests}/**/*.ts\"",
    "lint:fix": "eslint \"{src,tests}/**/*.ts\" --fix",
    "fmt": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "tc": "tsc --pretty --noEmit --project tsconfig.build.json",
    "prebuild": "rimraf lib",
    "build": "rollup -c rollup.config.ts --configPlugin swc3",
    "postbuild": "tsc --emitDeclarationOnly --declaration --declarationMap --project tsconfig.build.json",
    "dev": "rollup -c rollup.config.ts -w --configPlugin swc3",
    "test": "jest --verbose",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.24.4",
    "@swc/core": "^1.2.247",
    "@swc/jest": "^0.2.22",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.15",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "changesets": "^1.0.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest-extended": "^2.0.0",
    "jest": "^29.0.2",
    "jest-extended": "^3.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-swc3": "^0.5.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "oxide.ts": "^1.0.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "yarn@1.22.15"
}
